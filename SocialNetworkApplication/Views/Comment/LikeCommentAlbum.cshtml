@model IEnumerable<SocialNetworkApplication.Models.Comment_Like>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.timestamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.read)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment.comment1)
        </th>   
        <th>
            @Html.DisplayNameFor(model => model.Profile.first_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.read)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment.comment1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Profile.first_name)
        </td>
        <td>
            @Html.ActionLink("Mark as Read", "CommentLikeDetails", new { id = item.comment_id }, null)
        </td>
    </tr>
}

</table>
